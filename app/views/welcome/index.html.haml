= form_for("user", :url => user_session_path) do |f|
  .form-inputs
    = f.text_field :email, :autofocus => true,  placeholder: "Email", class: "form-control"
    = f.password_field :password, placeholder: "Senha", class: "form-control"
    = f.check_box :remember_me
  .form-actions
    = f.submit "Entrar", class: "btn-orange btn-block btn-lg"
- if current_user
  %h1 HOLA QTAL
